{
  "name": "logseq-atlas-theme",
  "version": "0.1.0",
  "description": "Clean theme for Logseq.",
  "author": "sethfair",
  "repo": "sethfair/logseq-atlas-theme",
  "main": "dist/index.html",
  "license": "MIT",
  "logseq": {
    "themes": [
      {
        "name": "Light",
        "url": "./custom.css",
        "mode": "light"
      }
    ],
    "icon": "./icon.png",
    "id": "logseq-atlas-theme"
  },
  "scripts": {
    "theme:build": "npx sass src/theme/main.scss custom.css --no-source-map",
    "theme:watch": "npx sass src/theme/main.scss custom.css --no-source-map --watch",
    "ui:build": "webpack --mode=production",
    "ui:watch": "webpack --watch --mode=development"
  },
  "dependencies": {
    "@logseq/libs": "^0.0.1-alpha.30",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "boxicons": "^2.0.9",
    "preact": "^10.5.15",
    "react-color": "^2.19.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@svgr/webpack": "^5.5.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "css-loader": "^6.5.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "sass": "^1.43.4",
    "style-loader": "^3.3.1",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  }
}
